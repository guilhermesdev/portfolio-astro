  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision",
        "@sanity/color-input"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    }
  ]
}
