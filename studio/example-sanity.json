{
  "root": true,
  "project": {
    "name": "YOUR_PROJECT_NAME"
  },
  "api": {
    "projectId": "YOUR_PROJECT_ID",
    "dataset": "YOUR_PROJECT_DATASET"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
